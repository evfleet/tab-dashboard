{
  "manifest_version": 2,
  "name": "Tab Extension",
  "version": "0.0.1",
  "description": "New tab extension",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["activeTab", "identity"],
  "minimum_chrome_version": "57"
}
